{
	"name": "visudata",
	"displayName": "visudata",
	"publisher": "krispyi2",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/crispyi2/visudata"
    },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:visudata.helloWorld"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "visudata.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode development --devtool hidden-source-map",
		"build": "mkdir -p dist && vsce package --out dist/test.vsix",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"coder-server": "sudo rm -r -f /home/coder/.local/share/code-server/extensions/visudata && sudo cp ./ /home/coder/.local/share/code-server/extensions/visudata"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
